<rdtk>
	<phase>
	    <scene name="dpVisServoPendulum"/>
	    <scene name="dpVisMagPendulum"/>
	    <!--<scene name="dpVisSandStorm"/>-->
	    <scene name="dpVisStruggle"/>
	    <scene name="dpVisTheta"/>
	    <scene name="dpHGearMove"/>
	    <scene name="dpHWorm"/>
	    <scene name="dpVisTornado"/>
	    <scene name="dpVisStage"/>
	    <!--<scene name="dpVisIce"/>-->
        <scene name="Future"/>
        <scene name="dpVisPLink_Laser"/>
        <scene name="dpVisPLink_Oil"/>
        <scene name="Line"/>
        <scene name="Four Points"/>
        <scene name="distanceMetaball"/>
        <scene name="Kioku"/>
        <!--<scene name="dpHEyeBallDancer"/>-->
	</phase>

	<nocam>
        <scene name="Future"/>
		<scene name="dpVisTheta"/>
		<scene name="dpHGearMove"/>
        <scene name="Line"/>
        <scene name="Four Points"/>
        <scene name="distanceMetaball"/>
        <!--<scene name="dpHEyeBallDancer"/>-->
	</nocam>
    
    <alloff>
        <scene name="Kioku"/>
    </alloff>
</rdtk>

<score>
	<complexity>
		 <scene name="dp::score::SceneVec2SimpleGraph"/>
		 <scene name="dp::score::SceneVec2Plotter"/>
         <scene name="dp::score::SceneDataText"/>
	</complexity>

	<complexity>
		<scene name="dp::score::SceneVec2Grid"/>
		<scene name="dp::score::SceneDataCircle"/>
        <scene name="dp::score::SceneDataSphere"/>
	</complexity>

	<complexity>
        <scene name="dp::score::SceneVec2Clocks"/>
		 <scene name="dp::score::SceneDataDisplacement"/>
		 <scene name="dp::score::SceneDataWave"/>
	</complexity>
    
    <body>
        <scene name="dp::score::SceneBodyScan"/>
        <scene name="dp::score::SceneBodyPatterns"/>
        <scene name="dp::score::SceneBodyGlobe"/>
        <scene name="dp::score::SceneBodyBoids"/>
        <scene name="dp::score::SceneBodyFlow"/>
    </body>
    
    <correlation>
        <scene name="dp::score::SceneCorrelation"/>
    </correlation>
</score>

